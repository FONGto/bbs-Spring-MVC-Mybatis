<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
 <mapper namespace="com.fong2.dao.PropertyDao">
 	<select id="findKeywordAndValue" resultType="com.fong2.entity.Property">
 		SELECT 
 			id as id,
 			keyword as keyword,
 			value as value 
 		FROM 
 			property
 	</select>
 
 </mapper>
